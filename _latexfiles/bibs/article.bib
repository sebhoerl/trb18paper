%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%  ARTICLE
%    An article from a journal or magazine.
%    Therefore, there MUST be a journal or a magazine in journal.txt,
%    i.e. Transportation; Transportation Research Record;
%    Zeitschrift für Verkehrswissenschaft; etc...
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%
%  KEY-DEFINITION:
%    (Author1|Author1Author2|Author1EtAl)_(JOURNAL)_(YEAR|forth)[_(INDEX)]
%    Example:
%      Axhausen_TPT_1996,
%      BalmerEtAl_ITS_2004,
%      BalmerEtAl_TRR_2006,
%      BalmerEtAl_TRR_forth,
%  AUTHOR
%    Must be crossrefs of authors given in the author.txt (add it there,
%    if one is missing)
%    Example:
%      AUTHOR =  Balmer # and # Nagel # and # Raney,
%  TITLE
%    Write the title in "Title-Style", the bib-style converts it.
%    If there are Characters which MUST be written as defined
%    (i.e. all-caps or no caps), then set those characters in
%    additional curly brackets.
%    Example:
%      TITLE =   {Large-Scale Multi-Agent Simulations for Transportation Applications {MATSim}},
%  JOURNAL
%    Must be a crossref of a jounal or magazine in the journal.txt (add
%    it there, if one is missing)
%    Example:
%      JOURNAL = ITS,
%  YEAR
%    Either the year of publication or set the crossref to 'forth'
%    as given in the translations.txt file
%    Example:
%      YEAR =    {2004},
%      YEAR =    forth,
%  volume
%    Optional. Write the volume number
%    Example:
%      volume =  {8},
%  number
%    Optional. Write the number (sometimes also called issue).
%    If you have a number a volume MUST be defined
%    Example:
%      number =  {4},
%  pages
%    Optional: Write the pages
%    Example:
%      pages =   {205--223},
%  comment
%    Optional: add any comment you want
%    Example:
%      comment =      {balmermi: this is a very good one.},
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

@ARTICLE{ARTICLE-AUTHOR_JOURNAL_YEAR-or-forth_INDEX,
  AUTHOR =  {ARTICLE-AUTHOR},
  TITLE =   {{TITLE}},
  JOURNAL = {JOURNAL},
  YEAR =    {0000},
  volume =  {volume},
  number =  {number},
  pages =   {pages},
  comment = {{comment}},
}

